{
    "applicationName":"Saml",

    "openIdProviderUrl":"https://%(hostname)s/.well-known/openid-configuration",
    "openIdClientId":"%(oxauth_client_id)s",
    "openIdClientPassword":"%(oxauthClient_encoded_pw)s",
    "openIdScopes":[
        "openid",
        "profile",
        "email",
        "user_name"
    ],
    "openIdRedirectUrl": "https://%(hostname)s/cas/login",
    "openIdPostLogoutRedirectUri": "https://%(hostname)s/identity/authentication/finishlogout",
    "openIdDefaultAuthenticator":true,
    "openIdClaimMapping":[
        {
            "claim":"email",
            "attribute":"email"
        },
        {
            "claim":"given_name",
            "attribute":"first_name"
        },
        {
            "claim":"family_name",
            "attribute":"family_name"
        },
        {
            "claim":"name",
            "attribute":"display_name"
        },
        {
            "claim":"locale",
            "attribute":"locale"
        },
        {
            "claim":"zoneinfo",
            "attribute":"zone"
        }
    ]
}
